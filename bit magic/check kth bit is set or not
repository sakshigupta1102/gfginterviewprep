import java.util.*;
import java.lang.*;
import java.io.*;

class GFG {
	public static void main (String[] args) {
		Scanner scan= new Scanner(System.in);
		int t=scan.nextInt();
		while(t>0){
		    int n=scan.nextInt();
		    int k= scan.nextInt();
		    int i=0;
		    while(i<k){
		        n=n>>1;
		        i++;
		    }
		    int p = n&1;
		    if(p==1){
		        System.out.println("Yes");
		    }else{
		        System.out.println("No");
		    }
		    
		    
		    
		    t--;
		}
	}
}
